<h1>Listing posts</h1>

<table>
  <tr>
    <th>Key</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th>Title</th>
    <th>Intro</th>
    <th>Date</th>
    <th>Dataini</th>
    <th>Datefin</th>
    <th>Follow</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.key %></td>
    <td><%= post.latitude %></td>
    <td><%= post.longitude %></td>
    <td><%= post.title %></td>
    <td><%= post.intro %></td>
    <td><%= post.date %></td>
    <td><%= post.dataini %></td>
    <td><%= post.datefin %></td>
    <td><%= post.follow %></td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= link_to 'Destroy', post, :method => :delete, :data => { :confirm => 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Post', new_post_path %>
